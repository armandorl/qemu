# See docs/devel/tracing.rst for syntax documentation.

# core.c

spi_event(const char *event, uint8_t address) "%s(addr:0x%02x)"
spi_send(uint8_t address, uint8_t data) "send(addr:0x%02x) data:0x%02x"
spi_send_async(uint8_t address, uint8_t data) "send_async(addr:0x%02x) data:0x%02x"
spi_recv(uint8_t address, uint8_t data) "recv(addr:0x%02x) data:0x%02x"
spi_ack(void) ""

